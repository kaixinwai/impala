{
  "extends": "../tsconfig-build",
  "compilerOptions": {
    "typeRoots": [
      "../../../node_modules/@types"
    ],
    "types": [
      "baidumap-web-sdk"
    ]
  },
  "files": [
    "public-api.ts",
    "../typings.d.ts"
